{
  "name": "@fultonjs/common",
  "version": "1.0.8",
  "description": "fultonjs common",
  "main": "index.js",
  "scripts": {
    "generate": "./node_modules/@babel/cli/bin/babel.js --copy-files --config-file ./babel.config.json  --out-dir lib src",
    "generateTest": "./node_modules/@babel/cli/bin/babel.js --config-file ./babel.config.json  --out-dir lib/test test",
    "generate2": "npx babel --config-file ./babel.config.json  --out-dir lib lib/es",
    "test": "echo \"Error: no test specified\" && exit 1",
    "genTypes": "npx tsc -p tsconfig.json &>/dev/null",
    "genFromTS": "npx tsc -p tsconfig-ts-to-es2015.json;: 'genereate *.js in place with *.ts'",
    "genFromTSDebug": "cp -R src/*.ts src/*.js src/debug/;cp -R src/protomock/ src/debug/protomock/;npx tsc -p tsconfig-ts-to-node-debug.json;: 'genereate *.js in place with *.ts, to src/debug'"
  },
  "author": "xhd2015",
  "license": "ISC",
  "dependencies": {
    "@babel/standalone": "^7.12.9",
    "@grpc/grpc-js": "^1.5.0",
    "@grpc/proto-loader": "^0.6.9",
    "axios": "^0.21.0",
    "base64url": "^3.0.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-oauth-server": "^2.0.0",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mongodb": "^3.6.4",
    "mysql": "^2.18.1",
    "nodejs-base64": "^1.0.3",
    "oauth2-server": "^3.1.1",
    "pm2": "^4.5.1",
    "protobufjs": "^6.11.2",
    "redis": "^3.0.2",
    "voca": "^1.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/generator": "^7.16.7",
    "@babel/preset-env": "^7.12.10",
    "@types/node": "^17.0.45",
    "typescript": "^4.3.4"
  },
  "eslintConfig": {
    "root": false
  }
}
